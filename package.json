{
  "name": "resium",
  "version": "1.4.1",
  "description": "React components for Cesium",
  "main": "dist/resium.cjs.js",
  "module": "dist/resium.es.js",
  "types": "dist/types/src/index.d.ts",
  "repository": "https://github.com/darwin-education/resium.git",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "clean": "del dist coverage examples/build",
    "clean:build": "del dist",
    "type": "tsc --noEmit --skipLibCheck",
    "lint": "eslint --ext '.js,.ts,.tsx' .",
    "build": "run-s clean:build build:es build:cjs build:umd build:umd:min",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:umd": "rollup -c",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c",
    "format": "eslint --ext '.js,.ts,.tsx' --quiet --fix .",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o .docz/dist/examples",
    "doc": "node docs/scripts/generateDoc && docz dev",
    "doc:build": "node docs/scripts/generateDoc && docz build && build-storybook -o .docz/dist/examples",
    "doc:generate": "node docs/scripts/generateDoc"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=8.3.0"
  },
  "peerDependencies": {
    "cesium": "1.x",
    "react": ">=16.3",
    "react-dom": ">=16.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-storysource": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/theming": "^5.0.11",
    "@types/cesium": "^1.54.4",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.12",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-loader": "^8.0.5",
    "cesium": "^1.57.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "docz": "^1.1.0",
    "docz-theme-default": "^1.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "jasmine-core": "^3.4.0",
    "jest": "^24.8.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.11.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0"
  }
}
